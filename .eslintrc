{
    "env": {
        "browser": true,
        "es6": true,
		"commonjs": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 9,
		"sourceType": "module",
		"parser": "babel-eslint"
    },
    "rules": {
		"semi": ["warn", "always"],
		"no-console": "off",
		"no-unused-vars": "warn"
    }
}